};

const whatsappNumber = '62895402343693';
const whatsappReal = encodeURIComponent('Halo, saya mau pesan Jasa Like Instagram Real Human');
const whatsappBot = encodeURIComponent('Halo, saya mau pesan Jasa Like Instagram Server/Bot');

---

<Layout metadata={metadata}>
