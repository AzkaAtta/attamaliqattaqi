      "offers": { "@type": "Offer", "priceCurrency": "IDR", "price": "20000", "availability": "https://schema.org/InStock", "url": "https://auradigital.id/layanan/jasa-like-instagram/" },
      "aggregateRating": { "@type": "AggregateRating", "ratingValue": "5", "reviewCount": "12" }
    },
    {
