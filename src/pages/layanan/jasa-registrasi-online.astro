---
import Layout from '~/layouts/PageLayout.astro';
import heroImage from '~/assets/images/jasa-registrasi-online.webp';

const metadata = {
  title: 'Registrasi Online Cepat & Aman — Jasa Profesional Indonesia',
  description:
    'Layanan jasa registrasi online untuk bisnis, event, atau sistem berbasis web. Proses cepat, aman, dan dibantu hingga data berhasil terdaftar.',
  keywords: [
    'registrasi online',
    'jasa registrasi web',
    'jasa registrasi sistem online',
    'registrasi online cepat',
    'registrasi online aman',
    'registrasi web perusahaan',
    'registrasi web toko online',
    'registrasi online sekolah',
    'layanan registrasi online',
    'registrasi online indonesia',
    'registrasi online terpercaya',
  ],
};

const whatsappNumber = '62895402343693';
const whatsappReal = encodeURIComponent(
  'Halo, saya mau menggunakan jasa registrasi online. Mohon info detail prosesnya.'
);
---

<Layout metadata={metadata}>
  <head>
    <link rel="canonical" href="https://auradigital.id/layanan/jasa-registrasi-online/" />
  </head>

  <section class="max-w-3xl mx-auto px-4 sm:px-6 lg:px-8 py-8 text-center">
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@graph": [
          {
            "@type": "WebSite",
            "@id": "https://auradigital.id/#website",
            "url": "https://auradigital.id/",
            "name": "AuraDigital",
            "inLanguage": "id-ID"
          },
          {
            "@type": "WebPage",
