          },
          {
            "@type": "Offer",
            "@id": "https://auradigital.id/layanan/jasa-registrasi-online#offer",
            "name": "Jasa Registrasi Online (Satuan)",
            "url": "https://auradigital.id/layanan/jasa-registrasi-online",
            "availability": "https://schema.org/InStock",
            "priceCurrency": "IDR",
            "price": "6000",
            "priceSpecification": {
              "@type": "UnitPriceSpecification",
              "priceCurrency": "IDR",
              "price": "6000",
              "unitCode": "EACH",
              "referenceQuantity": {
                "@type": "QuantitativeValue",
                "value": 1,
                "unitCode": "EACH"
              }
            },
